@using TravelTripProject.Models.Class
@model List<Blog>

<div class="padding1">
    <h4>Favori Yerlerim</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <a href="/Blog/BlogDetay/@x.ID"><img src="@x.BlogImage"  alt="" style="width:250px;height:90px;"/></a>
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,90)...</p>
            </div>
            <div class="clear"></div>
        }

    </div>
    @Html.Action("Partial3", "Default")
</div>
<div class="clear"></div>